{
    "name": "wickedone/phrase-translation-bundle",
    "description": "helpers to deal with your phrase translations",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WickedOne\\PhraseTranslationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WickedOne\\PhraseTranslationBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "wickedOne",
            "email": "wicliff.wolda@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.5.27",
        "infection/infection": "^0.26.16",
        "phpstan/phpstan": "^1.9.9",
        "phpstan/phpstan-deprecation-rules": "^1.1.1",
        "phpstan/phpstan-php-parser": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3.3",
        "roave/security-advisories": "dev-latest",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3"
    },
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "symfony/config": "^5.4.17|^6.0",
        "symfony/console": "^5.4.17|^6.0",
        "symfony/framework-bundle": "^5.4.17|^6.0",
        "symfony/http-client": "^5.4.17|^6.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/translation": "^5.4.17|^6.0",
        "vimeo/psalm": "^5.4"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
