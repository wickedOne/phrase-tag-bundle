{
    "name": "wickedone/phrase-tag-bundle",
    "description": "helpers to deal with your phrase translations",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WickedOne\\PhraseTagBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WickedOne\\PhraseTagBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "wickedOne",
            "homepage": "https://github.com/wickedOne"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "infection/infection": "^0.31",
        "matthiasnoback/symfony-dependency-injection-test": "^6.1.0",
        "phpstan/phpstan": "^2.1.29",
        "phpstan/phpstan-deprecation-rules": "^2.0.3",
        "phpstan/phpstan-phpunit": "^2.0.7",
        "phpunit/phpunit": "^10.5.11 || ^11.0 || ^12.0",
        "roave/security-advisories": "dev-latest"
    },
    "require": {
        "php": "~8.3.0 || ~8.4.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/console": "^6.4 || ^7.0",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/http-client": "^6.4 || ^7.0",
        "symfony/monolog-bundle": "^3.10.0",
        "symfony/translation": "^6.4 || ^7.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
