{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>wickedOne/.github:renovate-config"
  ],
  "rangeStrategy": "widen",
  "packageRules": [
    {
      "matchCategories": ["php"],
      "matchPackageNames": ["symfony/*"],
      "matchUpdateTypes": ["major"],
      "groupName": "symfony (major)",
      "groupSlug": "symfony-major"
    }
  ]
}